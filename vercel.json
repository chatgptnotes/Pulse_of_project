{
  "buildCommand": "cd apps/web && pnpm build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install -g pnpm@9 && pnpm install --no-frozen-lockfile",
  "framework": null,
  "functions": {
    "api/claude-handler.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/chat/message",
      "destination": "/api/claude-handler"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}