{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "api/claude-handler.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/chat/message",
      "destination": "/api/claude-handler"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}